#!/usr/bin/env bash
# This script lists files, removing part before first dash
for file in *
do
    # Skip hidden files
    if [[ "$file" =~ ^\. ]]; then
        continue
    fi
    
    # Check if file contains a dash
    if [[ "$file" == *-* ]]; then
        # Remove everything before and including the first dash
        echo "${file#*-}"
    else
        echo "$file"
    fi
done
